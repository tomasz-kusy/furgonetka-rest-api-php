{
  "name": "kwarcek/furgonetka-rest-api-php",
  "description": "Furgonetka REST API service in PHP.",
  "keywords": ["furgonetka","api","php","rest"],
  "homepage": "https://github.com/kwarcek/furgonetka-rest-api-php",
  "authors": [
    {
      "name": "Patryk Kania",
      "email": "ecollatio@gmail.com",
      "homepage": "https://github.com/kwarcek",
      "role": "Developer"
    }
  ],
  "license": [
    "MIT"
  ],
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.5 || ^7.4",
    "guzzlehttp/psr7": "^1.9",
    "fakerphp/faker": "^1.21"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "ramsey/uuid": "^4.7",
    "phpstan/phpstan": "^1.10"
  },
  "autoload-dev": {
    "psr-4": {
      "Kwarcek\\FurgonetkaRestApi\\Test\\": "tests"
    }
  },
  "autoload":{
    "psr-4":{
      "Kwarcek\\FurgonetkaRestApi\\":"src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
